{
    "name": "php-force/proxy-scrape-bundle",
    "type": "symfony-bundle",
    "description": "Proxy Scrape Bundle for Symfony",
    "keywords": ["proxy-scrape", "php-force", "symfony", "bundle"],
    "homepage": "https://github.com/php-force/proxy-scrape-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "PHP Force",
            "email": "php-force.assist@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpForce\\ProxyScrapeBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": "^7.4||^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "symfony/framework-bundle": "^4.4||^5.0",
        "symfony/yaml": "^4.4||^5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
